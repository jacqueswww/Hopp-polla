{
	"name": "ytfd",
	"version": "1.0",
	"background": {
		"scripts": ["background.js"]
	},
	"content_scripts": [{
		"js": ["contentscript.js"],
		"matches": ["https://www.youtube.com/*", "http://www.youtube.com/*"],
		"run_at": "document_end"
	}],
	"web_accessible_resources": ["script.js"],
	"permissions": [
		"tabs",
		"background",
	    "http://localhost:42050/*"
		//"http://www.youtube.com/*",
		//"https://www.youtube.com/*"
	],
	"manifest_version": 2
}
